   "source": [
    "# startswitch() 함수\n",
    "# 문자열의 접두사를 찾을 때 사용\n",
    "# 슬라이싱을 이용하는 대신 인덱스를 지정하지 않고 오류 발생 가능성이 낮다\n",
    "\n",
    "room = \"bedroom\"\n",
    "\n",
    "if room.startswith('bath'):\n",
    "    print(\"욕실입니다.\")\n",
    "elif room.startswith('bed'):\n",
    "    print(\"침실입니다.\")\n",
    "elif room.startswith('living'):\n",
    "    print(\"거실입니다.\")\n",
    "\n",
    "# 접미사를 찾기 위해서는 endswith() 함수를 이용"
   ]
  }
 ],
